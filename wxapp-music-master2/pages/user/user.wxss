page {
  background-color: #f1f1f1;
}
.avatar {
  height: 410rpx;
  background-color: #e21918;
  display: flex;
  justify-content: center;
  align-items: center;
}
.avatar open-data {
  width: 200rpx;
  height: 200rpx;
  border: 10rpx solid #e21918;
  border-radius: 50%;
  overflow: hidden;
}
.cell {
  font-size: 16px;
  background-color: #fff;
}
.cell-item {
  padding: 20rpx 30rpx;
  border-bottom: 1px solid #ddd;
}
.divider {
  height: 20rpx;
}